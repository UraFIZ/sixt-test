    "react-router-dom": "^5.2.0",
    "reselect": "^4.0.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "redux-devtools-extension": "2.13.8"
  },
